class ArrayWrapper {
    constructor(arr) {
        this.arr = arr;
    }

    valueOf() {
        // Enables addition via +
        return this.arr.reduce((sum, num) => sum + num, 0);
    }

    toString() {
        // Enables String(obj) to return "[...]" format
        return `[${this.arr.join(',')}]`;
    }
}
